		"angular": "~1.6.4",
		"angular-i18n": "~1.6.4",
		"angular-ui-router": "~0.4.2",
		"angular-resource": "~1.6.4",
		"angular-elastic": "^2.5.1",
		"materialize": "1.0.0",
		"jquery": "^3.2.1",
		"at-table": "^1.0.7",
		"glyphicons-only-bootstrap": "0.0.1"
	},
	"install": {
